{
  "name": "pim-apps-sdk",
  "version": "0.1.3",
  "description": "PIM Apps Software Developer Kit",
  "keywords": [
    "pim",
    "apps-sdk"
  ],
  "private": true,
  "main": "index.js",
  "author": "myles.systems",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulvollmer/pim-apps-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/paulvollmer/pim-apps-sdk/issues"
  },
  "homepage": "https://github.com/paulvollmer/pim-apps-sdk#readme",
  "scripts": {
    "test": "jest --coverage",
    "test-report": "jest --coverage --coverageReporters html",
    "lint": "eslint --ext .js .",
    "lint-fix": "eslint --fix --ext .js .",
    "format": "prettier --write *.js **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "pre-push": "pretty-quick --staged && npm run lint && npm run test"
    }
  },
  "devDependencies": {
    "eslint": "5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "express": "4.16.4",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "supertest": "3.4.2"
  }
}
